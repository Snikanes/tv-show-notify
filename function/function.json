{
    "disabled": false,
    "bindings": [
        {
            "name": "tvshows",
            "type": "blob",
            "direction": "in",
            "path": "tvshows/tvshows.json",
            "connection":"AzureWebJobsStorage"
        },
        {
            "name": "prefconfig",
            "type": "blob",
            "direction": "in",
            "path": "tvshows/prefconfig.json",
            "connection":"AzureWebJobsStorage"
        },
        {
            "schedule": "0 */5 * * * *",
            "name": "timer",
            "type": "timerTrigger",
            "direction": "in"
        }
    ]
}